<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>


<% @dares.each do |dare| %>
  <%= "#{dare.id}: #{dare.description}" %>
  <%= button_to "Upvote Button ", like_dare_path(dare), method: :put, class: "btn btn-success btn-xs" %> <span class="text-success"> (<%= dare.get_upvotes.size %>)</span>
  <br>
<% end %>



